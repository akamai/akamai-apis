  "paths": {
    "/{domain}/{resource}/{datacenterId}": {
      "post": {
        "description": "Use this action to submit load data. The URL specifies the `domain`, `resource`, and `datacenterId` of the instance to which the load data pertains. The data may be submitted in either XML or JSON format. To support legacy clients, we allow `region` as an alias for `datacenterId`. The timestamp string should be in XSD format.",
        "operationId": "post-domain-resource",
        "summary": "Submit load data",
        "tags": [
          "Load data"
        ],
        "externalDocs": {
          "description": "See documentation for this operation in Akamai's Global Traffic Management Load Feedback API",
          "url": "https://techdocs.akamai.com/gtm-load-feedback/reference/post-domain-resource"
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "current-load": 20,
                "datacenterId": 100,
                "domain": "example.akadns.net",
                "max-load": 30,
                "resource": "connections",
                "target-load": 25,
                "timestamp": "2015-05-01T19:38:53.188Z"
              },
              "schema": {
                "additionalProperties": false,
                "description": "Provides information that you need to specify when submitting load data for a GTM domain.",
                "type": "object",
                "required": [
                  "domain",
                  "datacenterId",
                  "resource",
                  "current-load",
                  "target-load",
                  "max-load",
                  "timestamp"
                ],
                "properties": {
                  "current-load": {
                    "description": "The current load on the resource in the given datacenter. For example, if the resource is database connections, and you have 50 connections, you would report 50 for this value.",
                    "example": "{{current-load}}",
                    "type": "integer"
                  },
                  "datacenterId": {
                    "description": "The number of the datacenter (traffic target) being updated. This number comes from the Akamai code field in Control Center. To support legacy clients, we accept `region` as an alias for `datacenterId`.",
                    "example": "{{datacenterId}}",
                    "type": "integer"
                  },
                  "domain": {
                    "description": "The name of the domain being updated, such as `example.akadns.net`.",
                    "example": "{{domain}}",
                    "type": "string"
                  },
                  "max-load": {
                    "description": "If the load in every datacenter exceeds its target, GTM raises targets proportional to the difference between `max-load` and `target-load`. Set `max-load` higher in data centers that can handle more traffic when the whole system is overloaded.",
                    "example": "{{max-load}}",
                    "type": "integer"
                  },
                  "resource": {
                    "description": "The name of the resource that is being used to balance load. This can be any quantity you define. For example, a resource can represent connections to a database, bandwidth, hits, or anything else.",
                    "example": "{{resource}}",
                    "type": "string"
                  },
                  "target-load": {
                    "description": "GTM's load balancer begins to shift traffic away from a traffic target when the `current-load` exceeds the `target-load`. For example, if you want traffic to begin shifting away from a database location when it has 100 connections, you would set the `target-load` to 100.",
                    "example": "{{target-load}}",
                    "type": "integer"
                  },
                  "timestamp": {
                    "description": "This should be the time at which the load was recorded, in XSD date format.",
                    "example": "{{timestamp}}",
                    "type": "string"
                  }
                },
                "x-akamai": {
                  "file-path": "schemas/domain-resource-dc-json.yaml"
                }
              }
            },
            "application/xml": {
              "example": "<load-object domain=\"example.akadns.net\" timestamp=\"2015-05-01T19:38:53.188Z\" version=\"1\" xmlns=\"http://www.akamai.com/FirstPoint/load-balancing\">\n    <datacenter datacenterId=\"100\">\n        <resource name=\"connections\">\n            <current-load>20</current-load>\n            <target-load>25</target-load>\n            <max-load>30</max-load>\n        </resource>\n    </datacenter>\n</load-object>",
              "schema": {
                "additionalProperties": false,
                "description": "Provides information that you need to specify when submitting load data for a GTM domain.",
