{
  "rules": {
    "name": "default",
    "children": [
      {
        "name": "Handle /my-path",
        "criteriaMustSatisfy": "all",
        "criteria": [
          {
            "name": "path",
            "value": [
              "/my-path"
            ]
          }
        ],
        "behaviors": [
          {
            "name": "caching",
            "behavior": "max-age",
            "ttl": "5m"
          }
        ]
      }
    ]
  }
}